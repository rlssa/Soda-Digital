@{
    ViewData["Title"] = "FAQ - Royal Life Saving eBook Store";
    Layout = "_Layout";
}


<div class="hero">
    <div class="space-separator--lg"></div>
    <div class="space-separator--lg"></div>
    <div class="space-separator--md"></div>
    <div class="space-separator"></div>
    <div class="page-container-sm">
        <div class="hero-container faq-hero-container">
            <h1 class="text-white hero-title-faq">FAQ</h1>
        </div>

    </div>
    <div class="space-separator--lg"></div>
    <div class="space-separator--lg"></div>
    <div class="space-separator--md"></div>
